- partial_data_src_url = url_for(user_organizations_path({:user_id => model.id, :skip_wrapper => true}))
%div.partial{:class => 'user-organization-partial-span', 'data-src' => partial_data_src_url}
  = render :partial => 'user_organizations/user_organization_list', :locals => {:models => (model.user_organizations), :user => model}
